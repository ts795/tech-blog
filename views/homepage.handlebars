<div class="container">
    {{#each this.posts }}
    <div class="row">
        <div class="card col-12 col-sm-12 m-2">
            <div class="card-header">
                {{ title }}
            </div>
            <div class="card-body">
                <p class="card-text">{{contents}}</p>
            </div>
        </div>
    </div>
    {{/each}}
</div>